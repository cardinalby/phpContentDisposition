{
    "name": "cardinalby/content-disposition",
    "version": "1.1.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cardinal",
            "homepage": "https://github.com/cardinalby/phpContentDisposition"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.19"
    },
    "autoload": {
        "psr-4": {
            "cardinalby\\ContentDisposition\\": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "cardinalby\\ContentDisposition\\Tests\\": "tests"
        }
    },
    "archive": {
        "exclude": [
            "/.github",
            "/tests",
            "/.gitattributes",
            "/.gitignore",
            "/phpunit.xml"
        ]
    }
}
